@{
    ViewData["Title"] = "Alexandru";
}



<!-- banner section -->
<section id="home" class="w3l-banner py-5">
    <div class="container p-5">

        <div class="row align-items-center">
            <div class="col-lg-6 col-sm-12 mt-lg-0 mt-4">
                <span class="title-small">Hello</span>
                <h1 class="mb-2 title"><span>I'm</span> Alexandru</h1>
                <h1 class="mb-4 title">
                    a <span class="typed-text"></span><span class="cursor">&nbsp</span>
                </h1>

                <p>
                    @* I'm Alexandru, *@
                    I'm a hardworking, team-oriented programmer who strives to write
                    clean, cache friendly code. I’m always eager to learn and leverage my skills
                    to facilitate unforgettable gameplay and narrative driven experiences.


                </p>
                <div class="mt-sm-5 mt-4">
                    <a onclick="location.href='@Url.Action("about", "Home")'" class="btn btn-primary btn-style mr-2">
                        Find More
                    </a>
                    @*<a onclick="location.href='@Url.Action("Projects", "Home")'" class="btn btn-outline-primary btn-style mr-2">
                    Portfolio
                    </a>*@
                </div>
            </div>
            <div class="col-lg-6 col-md-8 col-sm-10 mt-lg-0 mt-4">
                <div class="img-effect text-lg-center">
                    <img src="~/Img/photo.png"
                         alt="myphoto"
                         class="img-fluid" />
                </div>
            </div>
        </div>
    </div>
</section>
<!-- //banner section -->
<!-- home page about section -->
<section class="w3l-index3" id="about">
    <div class="midd-w3 py-5">
        <div class="container  p-5 py-lg-5 py-md-3">
            <div class="row">
                <div class="col-lg-4">
                    <div class="position-relative">
                        <img src="~/Img/myphoto.jpg"
                             alt=""
                             class="radius-image img-fluid" />
                    </div>
                </div>
                <div class="col-lg-1"></div>
                <div class="col-lg-7 mt-lg-0 mt-5">
                    <h5 class="title-small mb-2">Who am i?</h5>
                    <h3 class="title-big">
                        Someone great according to my son!
                    </h3>
                    @* <h3 class="title-big">
                    I'm Alexandru Lungu, a Game Developer and visual UI/UX Designer
                    </h3>*@
                    <p class="mt-4">
                        I value family very much and believe that things done with passion lead to infinity and beyond. That's why I tend to think that it is important that every person is allowed to develop his imagination because it can lead to great things.

                    </p>
                    <a href="https://drive.google.com/file/d/1LXZTyVKweYW7T2HDAFlz12weh-u52OSk/view?usp=sharing" target="_blank" class="btn btn-style btn-primary mt-lg-5 mt-4">Download CV</a>

                </div>
            </div>
        </div>
    </div>
</section>
<!-- //home page about section -->
<!-- home page services section -->
